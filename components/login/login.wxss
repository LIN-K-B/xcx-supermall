@import '/assets/iconfont/iconfont.wxss';

.login-container{
  height: 100vh;
  background-color: #ff8198;
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
}
.login-container-box{
  text-align: center;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%,-50%);
}
.login-container-box>view{
  padding: 10px 0;
}
.iconfont{
  font-size: 100px;
  color: rgb(68, 17, 30);
}
.tips-text{
  color: #666;
  font-size: 12px;
}
@import '/assets/iconfont/iconfont.wxss';
.detail-navbar{
  padding-top:20px ;
  display: flex;
  height: 46px;
  align-items: center;
  position: fixed;
  top: 0px;
  left: 0;
  right: 0;
  background-color: #fff;
  box-shadow: 0px 1px 2px rgb(0, 0, 0,.1);
  z-index: 1;
}
.detail-navbar-back{
  width: 80px;
  text-align: center;
}
.detail-navbar-title{
  display: flex;
  flex: 1;
}
.navbar-title-item{
  flex: 1;
}

.active{
  color: #ff8198;
}